{
  "manifest_version": 2,
  "name": "Password Strength Checker",
  "version": "1.0",
  "description": "Check the strength of passwords.",
  "permissions": [
    "activeTab"
  ],
  "browser_action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "icon16.png",
      "48": "icon48.png",
      "128": "icon128.png"
    }
  }
}
